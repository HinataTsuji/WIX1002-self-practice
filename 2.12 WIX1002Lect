import java.util.Random;
class Main {
    public static void main(String[] args) {
        int [][] shoeInventory = new int [5][2];
        Random g = new Random();
        for (int i=0; i<shoeInventory.length;i++){
            shoeInventory[i][0] = 6+i; //assign shoe size
            shoeInventory[i][1] = g.nextInt(11);
        }
        for (int i=0; i<shoeInventory.length;i++){
            int size = shoeInventory[i][0];
            int stock = shoeInventory[i][1];
            if (stock ==0)
                System.out.println("Size "+size+": Out of stock");
            else if (stock<3)
                System.out.println("Size "+size+": Low stock");
            else 
                System.out.println("Size "+size+": Available");
        }
    }
}
